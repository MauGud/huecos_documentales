{
  "version": 2,
  "builds": [
    {
      "src": "src/server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "src/server.js"
    },
    {
      "src": "/(.*\\.(js|css|png|jpg|jpeg|gif|svg|ico|woff|woff2|ttf|eot|json|xml))",
      "dest": "public/$1",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": "/styles.css",
      "dest": "public/styles.css"
    },
    {
      "src": "/app_new.js",
      "dest": "public/app_new.js"
    },
    {
      "src": "/(.*)",
      "dest": "src/server.js"
    }
  ]
}

